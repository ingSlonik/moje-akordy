{
  "name": "moje-akordy",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "start": "tsx server/index.tsx",
    "dev": "concurrently --kill-others \"npm run sever:dev\" \"npm run fe:dev\"",
    "sever:dev": "cross-env NODE_ENV=development tsx --watch server/index.tsx",
    "fe:dev": "cross-env NODE_ENV=development parcel src/index.html --no-cache",
    "build": "parcel build src/index.html",
    "deploy": "tsx deploy.ts"
  },
  "dependencies": {
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "easy-page-router": "^0.1.8",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "react": "^19",
    "react-dom": "^19",
    "sha1": "^1.1.1",
    "ssr-hook": "^0.2.0",
    "typed-client-server-api": "^1.0.3"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.13.3",
    "@parcel/transformer-sass": "^2.13.3",
    "@parcel/transformer-webmanifest": "^2.13.3",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sha1": "^1.1.5",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "node-ssh": "^13.2.0",
    "parcel": "^2.13.3",
    "process": "^0.11.10",
    "sharp": "^0.33.5",
    "tsx": "^4.19.3"
  }
}